/* components/icon-item/icon-item.wxss */

@import "/static/styles/style.wxss";

.image-cropper {
  position: relative;
  overflow: hidden;
  /* background-color: darkblue; */
}

.icon {
  /* align to image-cropper start */
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  position: absolute;
  /* align to image-cropper end */
  height: 100%;
  width: 100%;
}